<%= form_for @profile, :html => { :multipart => true } do |f| %>

  <p>
    <%= f.text_field :name %>
  </p>
  <div class="field">
    <%= f.label :avatar %>
    <%= f.file_field :avatar %>
  </div>
  <p>
    <%= f.text_field :location %><br>
  </p>
  <p>
    <%= f.text_field :biography %>
  </p>
  <p>
    <%= f.text_field :phone_number %>
  </p>
  <p>
  	<%= f.button :submit %>
  </p>
<% end %>

